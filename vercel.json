{
  "version": 2,
  "builds": [
    {
      "src": "./server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "./payment-project",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "./server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "./payment-project/$1"
    }
  ]
}

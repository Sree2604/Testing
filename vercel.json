{
  "version": 2,
  "builds": [
    {
      "src": "./server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "./payment-project",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "./server/index.js",
      "dest": "/api/(.*)"
    },
    {
      "src": "/payment-project/(.*)",
      "dest": "/"
    }
  ]
}

{
  "version": 2,
  "name": "demo-app",
  "builds": [
    {
      "src": "payment-project/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "payment-project/dist"
      }
    },
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1"
    }
  ]
}
